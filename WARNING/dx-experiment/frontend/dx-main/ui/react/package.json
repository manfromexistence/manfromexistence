{
  "name": "@dx-ui/www",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Make your ui beautifull.",
  "keywords": [
    "dx",
    "ui",
    "ux",
    "ai",
    "blockchain",
    "theming"
  ],
  "author": "ManOfExistence",
  "type": "module",
  "files": [
    "**/*.js",
    "**/*.js.map",
    "**/*.d.ts",
    "!**/*_test.*",
    "!.wireit/**"
  ],
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "default": "./index.js"
    }
  },
  "scripts": {
    "dev": "bun --hot index.tsx",
    "build": "wireit",
    "build:ts": "wireit",
    "prepublishOnly": "npm run build"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "build:ts"
      ]
    },
    "build:ts": {
      "command": "tsc --pretty",
      "files": [
        "tsconfig.json",
        "**/*.ts",
        "!**/*.d.ts"
      ],
      "output": [
        ".tsbuildinfo",
        "**/*.js",
        "**/*.js.map",
        "**/*.d.ts",
        "!*.js",
        "!types/"
      ],
      "clean": "if-file-deleted"
    }
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.21",
    "@types/react": "^16.9.53",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tsup": "^7.2.0",
    "@playwright/test": "^1.37.1",
    "prettier": "^2.2.1",
    "turbo": "^1.10.12",
    "@types/jasmine": "^3.10.3",
    "@types/node": "^18.7.17",
    "jasmine": "4.6.0",
    "typescript": "^4.5.5",
    "wireit": "^0.9.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "react": "^16.8 || ^17 || ^18",
    "react-dom": "^16.8 || ^17 || ^18"
  },
  "dependencies": {
    "@ant-design/nextjs-registry": "^1.0.0",
    "@nextui-org/system": "^2.0.15",
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "^3.3.2"
  }
}
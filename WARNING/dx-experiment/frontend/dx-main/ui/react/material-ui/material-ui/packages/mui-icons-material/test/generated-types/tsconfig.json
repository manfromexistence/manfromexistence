{
  "extends": "../../../../tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom"],
    "jsx": "react",
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "baseUrl": "../../../",
    "noErrorTruncation": true,
    "paths": {
      "@mui/base": ["./mui-base/src"],
      "@mui/base/*": ["./mui-base/src/*"],
      "@mui/docs": ["./mui-docs/src"],
      "@mui/docs/*": ["./mui-docs/src/*"],
      "@mui/icons-material": ["./mui-icons-material/build"],
      "@mui/icons-material/*": ["./mui-icons-material/build/*"],
      "@mui/joy": ["./mui-joy/src"],
      "@mui/joy/*": ["./mui-joy/src/*"],
      "@mui/lab": ["./mui-lab/src"],
      "@mui/lab/*": ["./mui-lab/src/*"],
      "@mui/internal-markdown": ["./markdown"],
      "@mui/internal-markdown/*": ["./markdown/*"],
      "@mui/material-next": ["./mui-material-next/src"],
      "@mui/material-next/*": ["./mui-material-next/src/*"],
      "@mui/material-nextjs": ["./mui-material-nextjs/src"],
      "@mui/material-nextjs/*": ["./mui-material-nextjs/src/*"],
      "@mui/material": ["./mui-material/src"],
      "@mui/material/*": ["./mui-material/src/*"],
      "@mui/private-theming": ["./mui-private-theming/src"],
      "@mui/private-theming/*": ["./mui-private-theming/src/*"],
      "@mui/styled-engine-sc": ["./mui-styled-engine-sc/src"],
      "@mui/styled-engine-sc/*": ["./mui-styled-engine-sc/src/*"],
      "@mui/styled-engine": ["./mui-styled-engine/src"],
      "@mui/styled-engine/*": ["./mui-styled-engine/src/*"],
      "@mui/styles": ["./mui-styles/src"],
      "@mui/styles/*": ["./mui-styles/src/*"],
      "@mui/system": ["./mui-system/src"],
      "@mui/system/*": ["./mui-system/src/*"],
      "@mui/types": ["./mui-types"],
      "@mui/utils": ["./mui-utils/src"],
      "@mui/utils/*": ["./mui-utils/src/*"],
      "@pigment-css/nextjs-plugin": ["./pigment-css-nextjs-plugin/src"],
      "@pigment-css/nextjs-plugin/*": ["./pigment-css-nextjs-plugin/src/*"],
      "@pigment-css/react": ["./pigment-css-react/src"],
      "@pigment-css/react/*": ["./pigment-css-react/src/*"],
      "@pigment-css/vite-plugin": ["./pigment-css-vite-plugin/src"],
      "@pigment-css/vite-plugin/*": ["./pigment-css-vite-plugin/src/*"],
      "@mui/internal-scripts/typescript-to-proptypes": [
        "../packages-internal/scripts/typescript-to-proptypes/src"
      ]
    }
  },
  "exclude": ["**/node_modules/"]
}

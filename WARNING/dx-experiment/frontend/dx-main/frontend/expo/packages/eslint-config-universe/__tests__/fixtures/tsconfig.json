{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["es2020"],
    "rootDir": "./",
    "baseUrl": "./",
    "noEmit": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
  }
}

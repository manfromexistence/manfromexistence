{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Dub.co Docs",
  "logo": {
    "dark": "/logos/logotype-dark.svg",
    "light": "/logos/logotype.svg",
    "href": "https://dub.co"
  },
  "favicon": "/logos/favicon.png",
  "colors": {
    "primary": "#9563FF",
    "light": "#AE87FF",
    "dark": "#0D001D",
    "background": {
      "dark": "#090014"
    },
    "anchors": {
      "from": "#FF7F57",
      "to": "#9563FF"
    }
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:support@dub.co"
    }
  ],
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://app.dub.co"
  },
  "openapi": "https://api.dub.co",
  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    }
  ],
  "anchors": [
    {
      "name": "Help Center",
      "icon": "question",
      "url": "https://dub.co/help"
    },
    {
      "name": "GitHub",
      "icon": "github",
      "url": "https://github.com/dubinc/dub"
    }
  ],
  "navigation": [
    {
      "group": "Getting Started",
      "pages": ["introduction", "local-development", "self-hosting"]
    },
    {
      "group": "Resources",
      "pages": ["integrations", "sdks", "examples"]
    },
    {
      "group": "API Reference",
      "pages": [
        "api-reference/introduction",
        "api-reference/rate-limits",
        "api-reference/errors"
      ]
    },
    {
      "group": "Links",
      "pages": [
        "api-reference/endpoint/create-a-new-link",
        "api-reference/endpoint/retrieve-a-link",
        "api-reference/endpoint/edit-a-link",
        "api-reference/endpoint/delete-a-link",
        "api-reference/endpoint/retrieve-a-list-of-links",
        "api-reference/endpoint/retrieve-number-of-links",
        "api-reference/endpoint/bulk-create-links"
      ]
    },
    {
      "group": "QR Codes",
      "pages": ["api-reference/endpoint/retrieve-a-qr-code"]
    },
    {
      "group": "Analytics",
      "pages": [
        "api-reference/endpoint/retrieve-clicks",
        "api-reference/endpoint/retrieve-timeseries",
        "api-reference/endpoint/retrieve-country",
        "api-reference/endpoint/retrieve-city",
        "api-reference/endpoint/retrieve-device",
        "api-reference/endpoint/retrieve-browser",
        "api-reference/endpoint/retrieve-os",
        "api-reference/endpoint/retrieve-referer",
        "api-reference/endpoint/retrieve-top-links",
        "api-reference/endpoint/retrieve-top-urls"
      ]
    },
    {
      "group": "Workspaces",
      "pages": [
        "api-reference/endpoint/create-a-new-workspace",
        "api-reference/endpoint/retrieve-a-workspace",
        "api-reference/endpoint/retrieve-a-list-of-workspaces"
      ]
    },
    {
      "group": "Tags",
      "pages": [
        "api-reference/endpoint/create-a-new-tag",
        "api-reference/endpoint/retrieve-a-list-of-tags"
      ]
    }
  ],
  "api": {
    "maintainOrder": true
  },
  "feedback": {
    "thumbsRating": true
  },
  "footerSocials": {
    "twitter": "https://twitter.com/dubdotco",
    "github": "https://github.com/steven-tey/dub",
    "linkedin": "https://www.linkedin.com/company/dubhq",
    "website": "https://dub.co"
  },
  "redirects": [
    {
      "source": "/integrations/:path*",
      "destination": "/integrations"
    },
    {
      "source": "/self-hosting/guide",
      "destination": "/self-hosting"
    },
    {
      "source": "/api-reference/endpoint/retrieve-a-project",
      "destination": "/api-reference/endpoint/retrieve-a-workspace"
    },
    {
      "source": "/api-reference/endpoint/retrieve-a-list-of-projects",
      "destination": "/api-reference/endpoint/retrieve-a-list-of-workspaces"
    }
  ]
}
